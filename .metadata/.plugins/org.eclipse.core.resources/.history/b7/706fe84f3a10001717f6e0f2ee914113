import java.util.*;


public class GreatGatsby 
{
	private static int chainOrder, numOfCharacters;
	private static String fileNameIn, fileNameOut;
	private static char current;
	
	public GreatGatsby(String one, int two, String three, int four)
	{
		//use buffered reader
		fileNameIn = one;
		chainOrder = two;
		fileNameOut = three;
		numOfCharacters = four;
	}
	
}